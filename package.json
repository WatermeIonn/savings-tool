{
  "name": "savings-tool",
  "author": "Daniel Cooper <daniellukecooper@outlook.com>",
  "version": "0.0.1",
  "private": true,
  "main": "./build/electron/main.js",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "build": {
    "files": [
      "dist/**/*",
      "package.json"
    ]
  },
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run dev\" \"wait-on http://127.0.0.1:3000 && tsc -p electron -w\" \"wait-on http://127.0.0.1:3000 && tsc -p electron && electron .\"",
    "electron:build": "npm run build && tsc -p electron && ./node_modules/.bin/electron-builder",
    "eject": "react-scripts eject",
    "typeorm": "node ./node_modules/typeorm/cli"
  },
  "dependencies": {
    "@heroui/button": "^2.2.24",
    "@heroui/input": "^2.4.25",
    "@heroui/modal": "^2.2.21",
    "@heroui/progress": "^2.2.20",
    "@heroui/react": "^2.8.2",
    "@heroui/spinner": "^2.2.21",
    "@heroui/system": "^2.4.20",
    "@heroui/table": "^2.2.24",
    "@heroui/theme": "^2.4.20",
    "@prisma/client": "^6.14.0",
    "@react-aria/ssr": "^3.9.10",
    "@react-aria/visually-hidden": "^3.8.26",
    "@tabler/icons-react": "^3.34.1",
    "@tailwindcss/postcss": "^4.1.12",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "class-transformer": "^0.5.1",
    "clsx": "^2.1.1",
    "decimal.js": "^10.6.0",
    "eslint": "9.34.0",
    "eslint-config-next": "15.5.0",
    "framer-motion": "^12.23.12",
    "intl-messageformat": "^10.7.16",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "prisma": "^6.14.0",
    "reflect-metadata": "^0.2.2",
    "tailwind-variants": "^3.0.0",
    "tailwindcss": "^4.1.12",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^4.0.0",
    "@types/node": "24.3.0",
    "@types/react": "19.1.11",
    "@types/react-dom": "19.1.7",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "electron-devtools-installer": "^4.0.0",
    "electron-reload": "^1.5.0",
    "next": "15.5.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "typescript": "^5.9.2",
    "wait-on": "^8.0.4",
    "webpack-filter-warnings-plugin": "^1.2.1"
  }
}
