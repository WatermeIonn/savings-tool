    "@nextui-org/link": "2.0.16",
    "@nextui-org/navbar": "2.0.16",
    "@nextui-org/react": "^2.1.10",
    "@nextui-org/snippet": "2.0.19",
    "@nextui-org/switch": "2.0.15",
    "@nextui-org/system": "2.0.5",
    "@nextui-org/theme": "2.1.3",
