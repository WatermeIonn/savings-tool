{
  "name": "savings-tool",
  "author": "Daniel Cooper <daniellukecooper@outlook.com>",
  "version": "0.0.1",
  "private": true,
  "main": "build/electron/main.js",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"cross-env BROWSER=none yarn dev\" \"wait-on http://127.0.0.1:3000 && tsc -p electron -w\" \"wait-on http://127.0.0.1:3000 && tsc -p electron && electron .\"",
    "electron:build": "yarn build && tsc -p electron && ./node_modules/.bin/electron-builder",
    "eject": "react-scripts eject",
    "typeorm": "node ./node_modules/typeorm/cli"
  },
